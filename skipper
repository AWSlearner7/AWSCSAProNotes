apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: skipper
  labels:
    application: hotels
  annotations:
    zalando.org/backend-weights: |
      {"go": 70, "java": 30}
spec:
  rules:
  - host: hotels.com
    http:
      paths:
      - backend:
          serviceName: go
          servicePort: http
        path: /
      - backend:
          serviceName: java
          servicePort: http
        path: /
