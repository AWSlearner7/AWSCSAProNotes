apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: hotels
  annotations:
    kubernetes.io/ingress.class: nginx
    nginx.ingress.kubernetes.io/canary: "true"
    nginx.ingress.kubernetes.io/canary-weight: "30"
spec:
  rules:
  - host: hotels.com
    http:
      paths:
      - backend:
          serviceName: java
          servicePort: 80
