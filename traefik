apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  annotations:
    traefik.ingress.kubernetes.io/service-weights: |
      go: 70%
      java: 30%
  name: traefik-web-ui
  namespace: kube-system
spec:
  rules:
  - host: traefik-ui.minikube
    http:
      paths:
      - backend:
          serviceName: go
          servicePort: 80
        path: /
      - backend:
          serviceName: java
          servicePort: 80
        path: /
