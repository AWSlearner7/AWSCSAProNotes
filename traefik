apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  annotations:
    kubernetes.io/ingress.class: traefik-external
    traefik.ingress.kubernetes.io/service-weights: |
      go: 70%
      java: 30%
  name: traefik
  namespace: default
spec:
  rules:
  - host: hotels.com
    http:
      paths:
      - backend:
          serviceName: go
          servicePort: 80
        path: /
      - backend:
          serviceName: java
          servicePort: 80
        path: /
status:
  loadBalancer: {}
